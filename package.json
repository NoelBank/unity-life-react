{
  "name": "unity-life-uikit",
  "version": "0.0.1",
  "description": "Uikit for Unity Life GTA 5 Project writen in React for visualization we use Storybook",
  "main": "./src/uikit/index.ts",
  "repository": "https://github.com/NoelBank/unity-life-uikit.git",
  "author": "Noel Bank <info@nbank.dev>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "gatsby": "2.25.3",
    "gatsby-plugin-sass": "2.4.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tsconfig-paths-webpack-plugin": "3.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-react": "7.12.5",
    "@babel/preset-typescript": "7.12.1",
    "@commitlint/cli": "11.0.0",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/react": "6.0.28",
    "@types/node": "^14.14.6",
    "@types/ragemp-c": "github:CocaColaBear/types-ragemp-c#v1.0",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/webpack": "4.41.24",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "babel-loader": "8.1.0",
    "babel-plugin-root-import": "6.6.0",
    "babel-plugin-universal-import": "3.1.3",
    "commitizen": "4.2.2",
    "concurrently": "5.3.0",
    "css-loader": "5.0.1",
    "eslint": "7.13.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "live-server": "1.2.1",
    "node-sass": "4.14.1",
    "plop": "2.7.4",
    "prettier": "2.1.2",
    "sass-loader": "10.0.5",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.10",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typed-scss-modules": "3.3.0",
    "typescript": "4.0.5",
    "webpack": "5.4.0",
    "webpack-cli": "4.2.0"
  },
  "scripts": {
    "generate-component": "plop component --plopfile plop/plopfile.js",
    "gc": "yarn generate-component",
    "scss-types": "tsm src/**/*.module.scss  --logLevel error --exportType default",
    "start:storybook": "concurrently \"yarn scss-types -w\" \"yarn start:storybook\" --names 'types,storybook'",
    "format:all": "prettier --write \"{src/**/*.@(js|jsx|ts|tsx|json|scss),./*.@(js|json)}\"",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  }
}
